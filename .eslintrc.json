{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": ["jsdoc"],
    "rules": {
    "import/no-extraneous-dependencies": [
    "error", {
       "devDependencies": true,
       "optionalDependencies": true,
       "peerDependencies": true,
       "packageDir": "./"
    }
],
     "linebreak-style": 0
    }

}
